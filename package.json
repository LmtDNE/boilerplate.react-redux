{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "A boiler plate with basic examples of react-router and react-redux",
  "scripts": {
    "start": "node server/server.js",
    "watch": "nodemon server/server.js --ignore ./client",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./client",
    "test": "./node_modules/.bin/mocha --compilers .:babel-core/register test/server test/client --recursive"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.0",
    "sinon": "^1.17.5"
  },
  "dependencies": {
    "babel-core": "^6.0.20",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babelify": "^7.3.0",
    "browserify-middleware": "^7.0.0",
    "express": "^4.14.0",
    "react": "^15.1.0",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1"
  }
}
